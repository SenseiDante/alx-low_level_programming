#include "main.h"

/**
 * _memcpy - function copies from source to destination
 * @src: memory source from copy
 * @dest: memory destination to copy
 * @n: number of bytes
 *
 * Return: copied memory with n byte changed
 */

char *_memcpy(char *dest, char *src, unsigned int n)
{
	int x = 0;
	int y = n;

	for (; x < y; x++)
	{
		dest[x] = src[x];
		n--;
	}
	return (dest);
}
